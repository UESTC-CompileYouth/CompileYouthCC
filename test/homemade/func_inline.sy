int gcd(int a, int b) {
    if (b == 0) { return a; }
    return gcd(b, a % b);
}

int while_f(int a) {
    int res = 0;
    while (a > 0) {
        res = res + a;
        a = a - 1;
    }
    return res;
}

int getstr(int get[]) {
    int x = getch();
    int length = 0;
    while (x != 13 && x != 10) {
        get[length] = x;
        length = length + 1;
        x = getch();
    }
    return length;
}

