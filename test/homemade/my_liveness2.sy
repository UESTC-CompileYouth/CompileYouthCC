int add(int a, int b, int c, int d,
int e, int f, int g, int h, int i, int j) {
    return a + b + c + d + e + f + g + h + i + j * 2;
}

int main() {
    int a = 1;
    int b = 2;
    int c = 3;
    int d = 4;
    int e = 5;
    int f = 6;
    int g = 7;
    int h = 8;
    int i = 9;
    int j = 10;

    int t = add(a, b, c, d, e, f, g, h, i, j);

    return t;
}